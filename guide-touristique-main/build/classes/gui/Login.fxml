<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane id="AnchorPane" fx:id="pane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #48d1cc;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.LoginController">
   <children>
      <Button fx:id="idConnecter" layoutX="297.0" layoutY="213.0" mnemonicParsing="false" onAction="#connecter" text="Se Connecter" />
      <Button fx:id="idSave" layoutX="309.0" layoutY="252.0" mnemonicParsing="false" onAction="#save" text="S'inscrire" />
      <TextField fx:id="idEmail" layoutX="236.0" layoutY="126.0" prefHeight="25.0" prefWidth="208.0" promptText="E-mail" style="-fx-background-color: String Email = rs.getString(2);;" />
      <PasswordField fx:id="idMotdepasse" layoutX="236.0" layoutY="173.0" prefHeight="25.0" prefWidth="208.0" promptText="Mot De Passe" />
      <Pane layoutY="-6.0" prefHeight="407.0" prefWidth="135.0" style="-fx-background-color: #e6e6fa;">
         <children>
            <ImageView fx:id="imageview" fitHeight="109.0" fitWidth="87.0" layoutX="24.0" layoutY="50.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/utilisateur.png" />
               </image>
            </ImageView>
            <ImageView fx:id="imagePwd" fitHeight="95.0" fitWidth="87.0" layoutX="24.0" layoutY="160.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/password.png" />
               </image>
            </ImageView>
         </children></Pane>
   </children>
</AnchorPane>
